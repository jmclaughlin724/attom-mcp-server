{
  "name": "attom-mcp-server",
  "version": "1.0.0",
  "description": "A comprehensive MCP server for ATTOM Data API with Docker support, caching, and fallback mechanisms.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "server": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:server": "ts-node src/server.ts",
    "docker:build": "docker build -t attom-mcp-server -f attom_mcp.dockerfile .",
    "docker:run": "./run_container.sh",
    "gen:openapi": "zod-openapi generate ./src/openapiRegistry.ts --output ./openapi/attom-api-schema.yaml",
    "test": "vitest"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^1.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "node-fetch": "^2.6.9",
    "yargs": "^17.7.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node-fetch": "^2.6.12",
    "@types/yargs": "^17.0.33",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  }
}
